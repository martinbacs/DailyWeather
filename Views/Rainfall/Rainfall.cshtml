@model RainfallModel
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
    ViewData["Title"] = "Rain";
    @Model.RainfallDataList[0].Value
*@
@{
        ViewData["Title"] = "Rainfall";

}


<h1>@ViewData["Title"]</h1>

<table>
    <tr>
        <th>Date</th>
        <th>Rainfall(mm)</th>
    </tr>

    @foreach (RainfallData data in Model.RainfallDataList)
    {
        <tr>
            <td>@data.Date</td>
            <td>@data.Value</td>
        </tr>
    }

</table>

